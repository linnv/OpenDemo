pstmt.reset(con->prepareStatement("CALL get_pop_continent(?,@pop)"));
for (int i=0; i<3; i++)
{
  pstmt->setString(1,cont_vector[i]);
  pstmt->execute();
  res.reset(stmt->executeQuery("SELECT @pop AS _population"));
  while (res->next())
    cout << "Population of "
         << cont_vector[i]
}
<< " is "
<< res->getString("_population") << endl;
